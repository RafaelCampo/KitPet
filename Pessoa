package Kitpet;

public class Pessoa {
	
	 String nome;
	 String endereco;
	 String telefone;
	 String cpf;
	 String telefoneCelular;
	 
	 void mostrarInformaç(){
			
			System.out.println("Nome do dono animal: "+nome);
			System.out.println("Endereço do dono do animal: "+endereco);
			System.out.println("Telefone residencial do dono do animal: "+telefone);
			System.out.println("CPF do dono do animal: "+cpf);
			System.out.println("Telefone celular do dono do animal: "+telefoneCelular);

		}

	
}
